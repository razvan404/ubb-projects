<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<connectionStrings>
		<clear/>
		<add name="ContestsDB" providerName="" connectionString="Data Source=RAZVAN\SQLEXPRESS;Initial Catalog=Contests;Integrated Security=True"/>
	</connectionStrings>
	<appSettings>
		<clear/>
		<add key="ParentTable" value="Organisers"/>
		<add key="ParentIDColumn" value="organiser_id"/>
		<add key="ChildTable" value="Contests"/>
		<add key="ChildIDColumn" value="contest_id"/>
		<add key="ChildColumns" value="name,type,subject,reservation_id,organiser_id"/>
		<add key="ChildParameters" value="@name,@type,@subject,@reservation_id,@organiser_id"/>
		<add key="ChildUpdateQuery" value="UPDATE Contests SET name = @name, type = @type, subject = @subject, reservation_id = @reservation_id WHERE contest_id = @contest_id"/>
		<add key="ChildDeleteQuery" value="DELETE FROM Contests WHERE contest_id = @contest_id"/>
	</appSettings>
</configuration>